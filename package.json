  "scripts": {
    "start:api": "env-cmd -f .env.api --silent nx serve api",
    "e2e:api": "nx e2e api-e2e --runInBand=true --cache=false --bail --skip-nx-cache --detectOpenHandles",
    "build:api": "nx build api --configuration=production --source-map=true",
    "migration:migrate:api": "node ./node_modules/env-cmd/bin/env-cmd -f ./.env.api ts-node --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run -d ./apps/api/src/app/modules/cores/database/providers/data-source-with-migration.provider.ts",
    "migration:create:api": "node ./node_modules/env-cmd/bin/env-cmd -f ./.env.api ts-node --project tsconfig.base.json ./scripts/create-api-migration.ts",
    "lint:all": "nx run-many --all --target=lint --skip-nx-cache",
    "prepare": "husky"
  },
  "private": true,
